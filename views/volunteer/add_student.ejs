<!--header -->
<%- include("include/_header") %>
<!-- add students -->
<div class="holder">
  <a href="/">
    <h2>List Users</h2>
  </a>
  <div class="form">
    <h3>ADD VOLU<span>N</span>TEER!</h3>
    <form onsubmit="addStudent()" id="addUser" action="/api/students" method="POST" class="_form" autocomplete="off">
      <div class="fields">
        <div class="_name">
          <label for="name"></label>
          <input type="text" name="name" id="name" placeholder="Name" />
          <input type="hidden" name="id" />
        </div>
        <div class="_age">
          <label for="age"></label>
          <input type="number" name="age" id="age" placeholder="Age" />
        </div>
        <div class="_livein">
          <label for="address"></label>
          <input type="text" name="address" id="address" placeholder="address" />
        </div>
        <div class="_phone">
          <label for="phone"></label>
          <input type="text" name="phone" id="phone" placeholder="+249 12 345 6789" />
        </div>
      </div>
      <div class="submitbotton">
        <input type="submit" />
        <input type="reset" />
      </div>
    </form>
  </div>

</div>
<!-- footer -->
<%- include("include/_footer") %>